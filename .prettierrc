{
	"semi": true,
	"singleQuote": true,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"printWidth": 120,
	"quoteProps": "consistent",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"singleAttributePerLine": true,
	"arrowParens": "always",
	"htmlWhitespaceSensitivity": "strict",
	"vueIndentScriptAndStyle": true
}
